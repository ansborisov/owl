{
    "name": "owl/owl",
    "type": "extension",
    "description": "Owl is an extreme fast PHP framework powered by Zephir language and delivered as C-extension.",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Patsura",
            "email": "talk@dmtry.me",
            "homepage": "http://dmtry.me/about"
        }
    ],
    "require": {
        "php": "~5.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phalcon/zephir": "dev-master",
        "phpunit/phpunit": "4.5",
        "codeception/codeception": "~2.0.12"
    },
    "suggest": {
        "ext-apc": "Allow using APC Cache\\Driver",
        "ext-memcache": "Allow using Memcache Cache\\Driver",
        "ext-memcached": "Allow using Memcached Cache\\Driver",
        "ext-phpredis": "Allow using Redis Cache\\Driver"
    }
}
